              "method": 2,
              "path": 3,
              "status_code": 4,
              "duration_ms": 5,
              "items_count": 6,
              "error.type": 7,
              "error.message": 8
            },
            "renameByName": {
              "@timestamp": "timestamp",
              "error.message": "error_message",
              "error.type": "error_type"
            }
          }
        },
